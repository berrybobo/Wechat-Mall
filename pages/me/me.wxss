.profile { padding: 40rpx 24rpx; display: flex; align-items: center; gap: 20rpx; background: #fff; }
.avatar { width: 96rpx; height: 96rpx; border-radius: 50%; background: #f5f5f5; }
.name { font-size: 30rpx; color: #111; }
.cells { margin-top: 16rpx; background: #fff; }
.cell { padding: 28rpx 24rpx; border-bottom: 1px solid #f0f0f0; }
